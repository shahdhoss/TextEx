{
    "manifest_version": 3,
    "name": "TextEx",
    "description": "Take a screenshot of any picture with text and convert it into copyable form immediately .",
    "permissions": [
        "activeTab",
        "http://127.0.0.1"  
    ],
    "version": "1.0",
    "action": {
      "default_popup": "backend/templates/popup.html",
      "default_icon": {
        "16":"assets/image.png",
        "24":"assets/image.png",
        "32":"assets/image.png"
        }
    }
  }